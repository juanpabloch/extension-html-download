{
  "manifest_version": 2,
  "name": "HTML Download",
  "version": "1.0",

  "description": "Download HTML content to .html file",
  "homepage_url": "https://github.com/juanpabloch/extension-html-download",
  "icons": {
    "48": "icons/html-color-100.png"
  },

  "permissions": ["activeTab"],

  "browser_action": {
    "default_icon": "icons/html-color-100.png",
    "default_title": "HTML Download",
    "default_popup": "popup/choose_beast.html"
  }
}
  
  